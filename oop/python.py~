class Animal:
	def __init__(self,name,color,age,size):
		self.name=name
		self.age=age
		self.color=color
		self.size=size

	def print_all(self):
		print(self.name)
		print(self.color)
		print(self.age)
		print(self.size)
		print(self.eat)
		print(self.sleep)

	def __init__(self,sleep,eat):
		self.sleep=sleep
		self.eat=eat


dog = Animal(name="max",color="black",age=4,size="huge",eat="is eating pizza",sleep="is sleeping")
cat = Animal(name="min",color="white",age=2,size="tiny",eat="is eating ice cream",sleep="is sleeping")

dog.print_all()
print("#########################")
cat.print_all()




